virt-install \
--name fed31 \
--ram 2048 \
--vcpus 1 \
--disk path=/var/lib/libvirt/images/fed31.img,size=20 \
--os-variant fedora31 \
--os-type linux \
--network bridge=br0 \
--graphics none \
--console pty,target_type=serial \
--location 'http://fedora.inode.at/releases/31/Server/x86_64/os/' \
--extra-args 'console=ttyS0,115200n8 serial'
